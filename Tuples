#Add an item

a=(4,6,2,8,3,1)
print(a)
a=a+(9,)
print(a)
a=a[:5]+(15,20,25)+a[:5]
print(a)
a=a+(30,)
print(a)

#Length of a tuple

a=('c','y','f','u','n','o')
print(a)
a=len(a)
print(a)

#Reverse a Tuple

a=('c','y','f','u','n','o')
a=a[::-1]
print(a)

#Repeated element in a tuple

a=(2, 4, 5, 6, 2, 3, 4, 4, 7)
print(a)
n=int(input())
print(a.count(n))
